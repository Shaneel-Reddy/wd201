<h2>My Todo List: </h2>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Due Date</th>
            <th>Completed</th>
        </tr>
    </thead>
    <tbody>
        <% allTodos.forEach(todo=>{%> 
            <tr>
                <td><%= todo.title %></td>
                <td><%= todo.dueDate %></td>
                <td><%= todo.completed ? 'Yes':'No' %></td>
            </tr>
        <%}); %>
    </tbody>
</table>